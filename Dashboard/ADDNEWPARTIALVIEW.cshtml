    function Purchase() {

        var POAprroved = [];
        var PORejected = [];
        var POPending = [];

        $.ajax({
            url: "../Dashboard/Purchase",
            data: { Fromdate: fromdate, Todate: todate },
            type: 'POST',
            dataType: 'json',
            success: function (result) {

                //$('#purchase').empty();


                for (i = 0; i < result.length; i++) {

                    POAprroved.push([result[i].POAprroved]);
                    PORejected.push([result[i].PORejected]);
                    POPending.push([result[i].POPending]);

                }

                var a11 = parseInt(POAprroved);
                var a21 = parseInt(PORejected);
                var a31 = parseInt(POPending);


                $("#myModal2").modal('hide');

                vibrant.setOptions({
                    colors: ['#CC6600', '#CC6600', '#CC6600', '#CC6600']
                });




                $('#purchase').vibrant({

                    chart: {
                        type: 'column'
                    },
                    title: {
                        text: ''
                    },
                    subtitle: {
                        text: ''
                    },
                    xAxis: {
                        type: 'category',
                        labels: {
                            rotation: -45,
                            style: {
                                fontSize: '08px',
                                fontFamily: 'Verdana, sans-serif'
                            }
                        }
                    },
                    yAxis: {
                        min: 0,
                        title: {
                            text: ''
                        }
                    },
                    legend: {
                        itemStyle: {
                            fontWeight: 'bold',
                            fontSize: '08px'
                        }
                    },
                    tooltip: {
                        pointFormat: '<b>{point.y:.1f}</b>'
                    },
                    series: [{
                        name: 'Purchase Order History',
                        data: [
                ['POAprroved', a11],
                ['PORejected', a21],
                ['POPending', a31]

            ],
                        dataLabels: {
                            enabled: true,
                            rotation: -90,
                            color: '#FFFFFF',
                            align: 'right',
                            format: '{point.y:.1f}', // one decimal
                            y: 10, // 10 pixels down from the top
                            style: {
                                fontSize: '08px',
                                fontFamily: 'Verdana, sans-serif'
                            }
                        }
                    }]
                });

            }

        });
    }


    function GRN() {
        $.ajax({
            url: "../Dashboard/GRN",
            data: { Fromdate: fromdate, Todate: todate },
            type: 'POST',
            dataType: 'json',
            success: function (result) {


                var res = [];

                var rest = { "name": "", "y": "", "x": "" };
                for (i = 0; i < result.length; i++) {
                    rest.name = result[i].NAME;
                    rest.y = result[i].Subtotal;
                    res.x = result[i].TotalGrnRecieved;
                    res.push(rest);
                    rest = { "name": "", "y": "", "x": "" };
                }

                $("#myModal2").modal('hide');

                vibrant.setOptions({
                    colors: ['rgb(15, 115, 255)', 'rgb(255, 82, 27)', 'rgb(139, 188, 33)', '#66CCFF', '#006B24', '#CC6600', '#64E572', '#FF9655', '#FFF263', '#6AF9C4']
                });



                $('#grn').vibrant({
                    chart: {

                        type: 'pie'
                    },
                    title: {
                        text: ''
                    },
                    yAxis: {
                        showEmpty: false
                    },
                    legend: {
                        itemStyle: {
                            fontWeight: 'bold',
                            fontSize: '08px'
                        }
                    },
                    plotOptions: {
                        pie: {
                            allowPointSelect: true,
                            cursor: 'pointer',
                            dataLabels: {
                                enabled: false
                            },
                            showInLegend: true
                        }
                    },
                    series: [{
                        name: "Value",
                        colorByPoint: true,
                        data: res
                    }]
                });


            }

        });
    }

