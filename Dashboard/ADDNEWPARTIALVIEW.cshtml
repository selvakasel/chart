            selectMonths: true,
            selectYears: true,
            editable: 'ray'

        });
        m = $(this).val();


    });
    var fromdate;
    var todate;
    $("#btnsav1").click(function () {
        $("#vali2").hide();
        fromdate = $("#frmtdate1").val();
        todate = $("#todate1").val();

        if (fromdate == '') {
            $("#vali2").show();
            $("#frmtdate1").focus();
            return false;
        }
        if (todate == '') {
            $("#vali2").show();
            $("#todate1").focus();
            return false;
        }

        if (parseInt(m) == 1) {
            Purchase();
            fromdate = $("#frmtdate1").val('');
            todate = $("#todate1").val('');
        }
        else if (parseInt(m) == 2) {
            GRN();
            fromdate = $("#frmtdate1").val('');
            todate = $("#todate1").val('');
        } else if (parseInt(m) == 3) {
            topboughtproducts()
            fromdate = $("#frmtdate1").val('');
            todate = $("#todate1").val('');

        } else if (parseInt(m) == 4) {
            Indentrequest();
            fromdate = $("#frmtdate1").val('');
            todate = $("#todate1").val('');

        } else if (parseInt(m) == 5) {
            Sales();
            fromdate = $("#frmtdate1").val('');
            todate = $("#todate1").val('');

        } else if (parseInt(m) == 6) {
            Income();
            fromdate = $("#frmtdate1").val('');
            todate = $("#todate1").val('');

        } else if (parseInt(m) == 7) {
            Loss();
            fromdate = $("#frmtdate1").val('');
            todate = $("#todate1").val('');

        } else if (parseInt(m) == 8) {
            TosalesProduct();
            fromdate = $("#frmtdate1").val('');
            todate = $("#todate1").val('');

        }
        m = 0;
        $("#myModal2").modal('hide');
    });

