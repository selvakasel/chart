        GRN();
        topboughtproducts()
        Indentrequest();
        Sales();
        Income();
        Loss();
        TosalesProduct();
        $("#myModalss").modal('hide');
    });

    var m = 0;
    $("#but1,#but2,#but3,#but4,#but5,#but6,#but7,#but8").click(function () {

        $("#myModal2").modal('show');
        $("#myModal2").modal({ backdrop: false });

        $("#frmtdate1").pickadate({
            format: "dd/mm/yyyy",
            // min: true,
