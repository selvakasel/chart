<script type="text/javascript">
    function Currentstockdetails() {
        window.open("../HmsWebPages/InventoryRpt/Currentstockdetailsfrm.aspx?ReportName=" + "rptcurrentstockdash.rpt");
    }
    function quantityprise() {
