<script type="text/javascript">

    var m = 0;
    $("#but1,#but2,#but3,#but4").click(function () {

        $("#myModal2").modal('show');
        $("#myModal2").modal({ backdrop: false });

        $("#frmtdate1").pickadate({
            format: "dd/mm/yyyy",
            // min: true,
            selectMonths: true,
            selectYears: true,
            editable: 'ray'

        });
        $("#todate1").pickadate({
            format: "dd/mm/yyyy",
            //  min: true,
            selectMonths: true,
            selectYears: true,
            editable: 'ray'

        });
        m = $(this).val();


    });
    var fromdate;
    var todate;
    $("#btnsav1").click(function () {
        $("#vali2").hide();
        fromdate = $("#frmtdate1").val();
        todate = $("#todate1").val();

        if (fromdate == '') {
            $("#vali2").show();
            $("#frmtdate1").focus();
            return false;
        }
        if (todate == '') {
            $("#vali2").show();
            $("#todate1").focus();
            return false;
        }

        if (parseInt(m) == 1) {
            CurrentstockProduct();
            fromdate = $("#frmtdate1").val('');
            todate = $("#todate1").val('');
        }
        else if (parseInt(m) == 2) {
            Quantityandsales();
            fromdate = $("#frmtdate1").val('');
            todate = $("#todate1").val('');
        } else if (parseInt(m) == 3) {
            Storewisecurrentstock()
            fromdate = $("#frmtdate1").val('');
            todate = $("#todate1").val('');

        } else if (parseInt(m) == 4) {
            Salesvalue();
            fromdate = $("#frmtdate1").val('');
            todate = $("#todate1").val('');
        }
        m = 0;
        $("#myModal2").modal('hide');
    });

    $("#cancel2").click(function () {
        $("#myModal2").modal('hide');
    });


    function CurrentstockProduct() {

        var PurchasePrice = [];
        var Salesprice = [];


        $.ajax({
            url: "../Dashboard/currentstock1",
            data: { Fromdate: fromdate, Todate: todate },
            type: 'POST',
            dataType: 'json',
            success: function (result) {


                for (i = 0; i < result.length; i++) {

                    PurchasePrice.push([result[i].Clientname]);
                    Salesprice.push([result[i].AmountPaid]);

                }


                vibrant.setOptions({
                    colors: ['#CC6600', '#CC6600', '#CC6600', '#CC6600']
                });

                $('#gpprayan').vibrant({

                    chart: {
                        type: 'column'
                    },
                    title: {
                        text: ''
                    },
                    subtitle: {
                        text: ''
                    },
                    xAxis: {
                        category: PurchasePrice,
                        labels: {
                            rotation: -45,
                            style: {
                                fontSize: '08px',
                                fontFamily: 'Verdana, sans-serif'
                            }
                        }
                    },
                    yAxis: {
                        min: 0,
                        title: {
                            text: ''
                        }
                    },
                    legend: {
                        itemStyle: {
                            fontWeight: 'bold',
                            fontSize: '08px'
                        }
                    },
                    tooltip: {
                        pointFormat: '<b>{point.y:.1f}</b>'
                    },
                    series: [{
                        name: 'MainAvailableQty',
                        data: [Salesprice],
                        dataLabels: {
                            enabled: true,
                            rotation: -90,
                            color: '#FFFFFF',
                            align: 'right',
                            format: '{point.y:.1f}', // one decimal
                            y: 10, // 10 pixels down from the top
                            style: {
                                fontSize: '08px',
                                fontFamily: 'Verdana, sans-serif'
                            }
                        }
                    }]
                });
            }
        });
    }



    function Quantityandsales() {

        $.ajax({
            url: "../Dashboard/Quantityrate1",
            data: { Fromdate: fromdate, Todate: todate },
            type: 'POST',
            dataType: 'json',
            success: function (result) {
                var series2 = [];
                var Assignedto = [];
                var series3 = [];


                for (i = 0; i < result.length; i++) {

                    Assignedto.push([result[i].Clientname]);
                    series2.push([result[i].AmountPaid]);
                    series3.push([result[i].Amount]);


                }


                $('#gppra').vibrant({
                    chart: {
                        type: 'spline'
                    },
                    title: {
                        text: ''
                    },
                    subtitle: {
                        text: ''
                    },
                    xAxis: {
                        categories: Assignedto
                    },
                    yAxis: {
                        title: {
                            text: ''
                        },
                        labels: {
                            formatter: function () {
                                return this.value + '';
                            }
                        }
                    },
                    tooltip: {
                        crosshairs: true,
                        shared: true
                    },
                    plotOptions: {
                        spline: {
                            marker: {
                                radius: 4,
                                lineColor: '#666666',
                                lineWidth: 1
                            }
                        }
                    },
                    series: [{
                        name: 'Quantity',
                        marker: {
                            symbol: 'square'
                        },
                        data: series2

                    }, {
                        name: 'Rate',
                        marker: {
                            symbol: 'diamond'
                        },
                        data: series3
                    }]
                });

            }
        });
    }









    function Storewisecurrentstock() {
        $.ajax({
            url: "../Dashboard/Storewisecurrentstock1",
            data: { Fromdate: fromdate, Todate: todate },
            type: 'POST',
            dataType: 'json',
            success: function (result) {

                var res = [];

                debugger
                var rest = { "name": "", "y": "" };
                for (i = 0; i < result.length; i++) {
                    rest.name = result[i].BaseEntityName;
                    rest.y = result[i].Amount;
                    res.push(rest);
                    rest = { "name": "", "y": "" };
                }

                vibrant.setOptions({
                    colors: ['rgb(15, 115, 255)', 'rgb(255, 82, 27)', 'rgb(139, 188, 33)', '#66CCFF', '#006B24', '#CC6600', '#64E572', '#FF9655', '#FFF263', '#6AF9C4']
                });

                $('#gpan').vibrant({
                    chart: {

                        type: 'pie'
                    },
                    title: {
                        text: ''
                    },
                    yAxis: {
                        showEmpty: false
                    },
                    legend: {
                        itemStyle: {
                            fontWeight: 'bold',
                            fontSize: '08px'
                        }
                    },
                    plotOptions: {
                        pie: {
                            allowPointSelect: true,
                            cursor: 'pointer',
                            dataLabels: {
                                enabled: false
                            },
                            showInLegend: true
                        }
                    },
                    series: [{
                        name: "Value",
                        colorByPoint: true,
                        data: res
                    }]
                });


            }
        });


    }


    function Salesvalue() {

        $.ajax({
            url: "../Dashboard/salesvalue1",
            data: { Fromdate: fromdate, Todate: todate },
            type: 'POST',
            dataType: 'json',
            success: function (result) {
                var series2 = [];
                var Assignedto = [];
                var res = [];

                for (i = 0; i < result.length; i++) {

                    series2.push([result[i].BaseEntityName]);
                    Assignedto.push([result[i].AmountPaid]);
                    res.push([result[i].InvoiceBalance]);
                }

                $('#grayan').vibrant({
                    chart: {
                        type: 'area',
                        inverted: true
                    },
                    title: {
                        text: ''
                    },
                    subtitle: {
                        style: {
                            position: 'absolute',
                            right: '0px',
                            bottom: '10px'
                        }
                    },
                    legend: {
                        layout: 'vertical',
                        align: 'right',
                        verticalAlign: 'top',
                        x: -150,
                        y: 100,
                        floating: true,
                        borderWidth: 1,
                        backgroundColor: (vibrant.theme && vibrant.theme.legendBackgroundColor) || '#FFFFFF'
                    },
                    xAxis: {
                        categories: series2
                    },
                    yAxis: {
                        title: {
                            text: ''
                        },
                        labels: {
                            formatter: function () {
                                return this.value;
                            }
                        },
                        min: 0
                    },
                    plotOptions: {
                        area: {
                            fillOpacity: 0.5
                        }
                    },
                    series: [{
                        name: 'SalesPrice',
                        data: Assignedto
                    }, {
                        name: 'Total',
                        data: res
                    }]
                });

            }
        });

    }

  </script>

