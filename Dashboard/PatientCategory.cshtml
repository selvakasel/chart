<script type="text/javascript">  


    var m = 0;
    $("#but1,#but2,#but3,#but4").click(function () {

        $("#myModal2").modal('show');
        $("#myModal2").modal({ backdrop: false });

        $("#frmtdate1").pickadate({
            format: "dd/mm/yyyy",
            // min: true,
            selectMonths: true,
            selectYears: true,
            editable: 'ray'

        });
        $("#todate1").pickadate({
            format: "dd/mm/yyyy",
            //  min: true,
            selectMonths: true,
            selectYears: true,
            editable: 'ray'

        });
        m = $(this).val();


    });
    var fromdate;
    var todate;
    $("#btnsav1").click(function () {
        $("#vali2").hide();
        fromdate = $("#frmtdate1").val();
        todate = $("#todate1").val();

        if (fromdate == '') {
            $("#vali2").show();
            $("#frmtdate1").focus();
            return false;
        }
        if (todate == '') {
            $("#vali2").show();
            $("#todate1").focus();
            return false;
        }

        if (parseInt(m) == 1) {
            paitiant1();
            fromdate = $("#frmtdate1").val('');
            todate = $("#todate1").val('');
        }
        else if (parseInt(m) == 2) {
            paitiant2();
            fromdate = $("#frmtdate1").val('');
            todate = $("#todate1").val('');
        } else if (parseInt(m) == 3) {
            paitiant3();
            
            fromdate = $("#frmtdate1").val('');
            todate = $("#todate1").val('');

        } else if (parseInt(m) == 4) {
            paitiant4();
            fromdate = $("#frmtdate1").val('');
            todate = $("#todate1").val('');

        }
        m = 0;
        $("#myModal2").modal('hide');
    });

    $("#cancel2").click(function () {
        $("#myModal2").modal('hide');
    });
</script>

