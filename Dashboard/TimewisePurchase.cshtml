<script type="text/javascript">
    var m = 0;
    $("#but1,#but2,#but3,#but4").click(function () {

        $("#myModal2").modal('show');
        $("#myModal2").modal({ backdrop: false });

        $("#frmtdate1").pickadate({
            format: "dd/mm/yyyy",
            // min: true,
            selectMonths: true,
            selectYears: true,
            editable: 'ray'

        });
        $("#todate1").pickadate({
            format: "dd/mm/yyyy",
            //  min: true,
            selectMonths: true,
            selectYears: true,
            editable: 'ray'

        });
        m = $(this).val();


    });
    var fromdate;
    var todate;
    $("#btnsav1").click(function () {
        $("#vali2").hide();
        fromdate = $("#frmtdate1").val();
        todate = $("#todate1").val();

        if (fromdate == '') {
            $("#vali2").show();
            $("#frmtdate1").focus();
            return false;
        }
        if (todate == '') {
            $("#vali2").show();
            $("#todate1").focus();
            return false;
        }

        if (parseInt(m) == 1) {
            PoPending1();
            fromdate = $("#frmtdate1").val('');
            todate = $("#todate1").val('');
        }
        else if (parseInt(m) == 2) {
            PORejected1();
            fromdate = $("#frmtdate1").val('');
            todate = $("#todate1").val('');
        } else if (parseInt(m) == 3) {
            Indentrequest()
            fromdate = $("#frmtdate1").val('');
            todate = $("#todate1").val('');

        } else if (parseInt(m) == 4) {
            topboughtproducts();
            fromdate = $("#frmtdate1").val('');
            todate = $("#todate1").val('');
        }
        m = 0;
        $("#myModal2").modal('hide');
    });

    $("#cancel2").click(function () {
        $("#myModal2").modal('hide');
    });


    function topboughtproducts() {
        $.ajax({
            url: "../Dashboard/topboughtproductsdash",
            data: { Fromdate: fromdate, Todate: todate },
            type: 'POST',
            dataType: 'json',
            success: function (result) {
                var res = [];


                var rest = { "name": "", "y": "" };
                for (i = 0; i < result.length; i++) {
                    rest.name = result[i].Productname;
                    rest.y = result[i].TotalCOunt;
                    res.push(rest);
                    rest = { "name": "", "y": "" };
                }

               
                vibrant.setOptions({
                    colors: ['rgb(15, 115, 255)', 'rgb(255, 82, 27)', 'rgb(139, 188, 33)', '#66CCFF', '#006B24', '#CC6600', '#64E572', '#FF9655', '#FFF263', '#6AF9C4']
                });

                $('#closed').vibrant({
                    chart: {
                        type: 'pie'
                    },
                    title: {
                        text: ''
                    },
                    yAxis: {
                        showEmpty: false
                    },
                    legend: {
                        itemStyle: {
                            fontWeight: 'bold',
                            fontSize: '08px'
                        }
                    },
                    plotOptions: {
                        pie: {
                            allowPointSelect: true,
                            cursor: 'pointer',
                            dataLabels: {
                                enabled: false
                            },
                            showInLegend: true
                        }
                    },
                    series: [{
                        name: "Value",
                        colorByPoint: true,
                        data: res
                    }]
                });



            }
        });
    }

    function Indentrequest() {
        $.ajax({
            url: "../Dashboard/Indentrequestdashw",
            data: { Fromdate: fromdate, Todate: todate },
            type: 'POST',
            dataType: 'json',
            success: function (result) {
                var res = [];

                debugger
                var rest = { "name": "", "y": "" };
                for (i = 0; i < result.length; i++) {
                    rest.name = result[i].SubStoreName;
                    rest.y = result[i].TotalRequest;
                    res.push(rest);
                    rest = { "name": "", "y": "" };
                }

                $("#myModal2").modal('hide');
                vibrant.setOptions({
