                $('#Indentrequest').vibrant({
                    chart: {

                        type: 'pie'
                    },
                    title: {
                        text: ''
                    },
                    yAxis: {
                        showEmpty: false
                    },
                    legend: {
                        itemStyle: {
                            fontWeight: 'bold',
                            fontSize: '08px'
                        }
                    },
                    plotOptions: {
                        pie: {
                            allowPointSelect: true,
                            cursor: 'pointer',
                            dataLabels: {
                                enabled: false
                            },
                            showInLegend: true
                        }
                    },
                    series: [{
                        name: "Value",
                        colorByPoint: true,
                        data: res
                    }]
                });


            }
        });

    }





    function PoPending1() {

        $.ajax({
            url: "../Dashboard/Timewisestore1",
            data: { Fromdate: fromdate, Todate: todate },
            type: 'POST',
            dataType: 'json',
            success: function (result) {
                var series2 = [];
                var Assignedto = [];


                for (i = 0; i < result.length; i++) {

                    series2.push([result[i].Clientname]);

                    Assignedto.push([result[i].BillAmount]);
                }


                vibrant.setOptions({
                    colors: ['rgb(15, 115, 255)', 'rgb(255, 82, 27)', 'rgb(139, 188, 33)', '#66CCFF', '#006B24', '#CC6600', '#64E572', '#FF9655', '#FFF263', '#6AF9C4']
                });


                $('#totalasset1').vibrant({
                    chart: {
                        type: 'column',
                        options3d: {
                            enabled: true,
                            alpha: 10,
                            beta: 25

                        }

                    },
                    title: {
                        text: ''
                    },
                    xAxis: {
                        categories: series2,
                        labels: {
                            rotation: -70,
                            style: {
                                font: '08px Helvetica'
                            }
                        }

                    },
                    yAxis: {

                        title: {
                            text: '',
                            align: 'high'
                        }

                    },

                    plotOptions: {
                        column: {
                            dataLabels: {
                                enabled: true
                            }
                        }
                    },
                    legend: {

                        align: 'right',
                        verticalAlign: 'top',
                        x: -80,
                        y: 2,
                        floating: true,
                        backgroundColor: ((vibrant.theme && vibrant.theme.legendBackgroundColor) || '#FFFFFF'),
                        shadow: false,
                        enabled: false
                    },
                    credits: {
                        enabled: false
                    },
                    series: [{
                        name: "Value",
                        data: Assignedto
                    }]
                });


            }
        });

    }



    function PORejected1() {
        $.ajax({
            url: "../Dashboard/dayTimesubstore1",
            data: { Fromdate: fromdate, Todate: todate },
            type: 'POST',
            dataType: 'json',
            success: function (result) {

