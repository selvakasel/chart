                for (i = 0; i < result.length; i++) {

                    SubStoreName.push([result[i].Clientname]);
                    InvoiceSubTotal.push([result[i].BillAmount]);

                }




                $('#container').vibrant({

                    chart: {
                        type: 'column'
                    },
                    title: {
                        text: ''
                    },
                    subtitle: {
                        text: ''
                    },
                    xAxis: {
                        categories: SubStoreName,
                        labels: {
                            rotation: -45,
                            style: {
                                fontSize: '08px',
                                fontFamily: 'Verdana, sans-serif'
                            }
                        }
                    },
                    yAxis: {
                        min: 0,
                        title: {
                            text: ''
                        }
                    },
                    legend: {
                        itemStyle: {
                            fontWeight: 'bold',
                            fontSize: '08px'
                        }
                    },
                    tooltip: {
                        pointFormat: '<b>{point.y:.1f}</b>'
                    },
                    series: [{
                        data:
                    InvoiceSubTotal,

                        dataLabels: {
                            enabled: true,
                            rotation: -90,
                            color: '#FFFFFF',
                            align: 'right',
                            format: '{point.y:.1f}', // one decimal
                            y: 10, // 10 pixels down from the top
                            style: {
                                fontSize: '08px',
                                fontFamily: 'Verdana, sans-serif'
                            }
                        }
                    }]
                });

            }

        });

    }  

  </script>


