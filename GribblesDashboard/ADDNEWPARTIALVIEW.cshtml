    function Racepatientsfun() {

        $.ajax({
            url: "../Store/GribblesDashboard/Racepatients",
            data: { Fromdate: fromdateconv(fromdate), Todate: todateconv(todate) },
            type: 'POST',
            dataType: 'json',
            success: function (result) {



                var res = [];

                var rest = { "name": "", "y": "" };
                for (i = 0; i < result.length; i++) {
                    rest.name = result[i].BaseEntityName;
                    rest.y = result[i].BedID;
                    res.push(rest);
                    var d = res;
                    rest = { "name": "", "y": "" };


                }

                vibrant.setOptions({
                    colors: ['rgb(139, 188, 33)', '#FF0000', '#00FFFF', '#800080', '#00FF00', '#FF00FF', '#0000A0', '#A52A2A']
                });



                $('#racepatientsid1').vibrant({
                    chart: {

                        type: 'pie',
                        height: 220
                    },
                    title: {
                        text: ''
                    },
                    yAxis: {
                        showEmpty: false
                    },
                    legend: {
                        itemStyle: {
                            fontWeight: 'bold',
                            fontSize: '10px'
                        }
                    },
                    plotOptions: {
                        pie: {
                            allowPointSelect: true,
                            cursor: 'pointer',
                            dataLabels: {
                                enabled: true,
                                format: ' {point.y:1f}'
                            },
                            showInLegend: true
                        }
                    },
                    series: [{
                        name: "Race",
                        colorByPoint: true,
                        data: res
                    }]



                });

            }

        });
    }




    function Shareingribblehospitalfun(drop) {

        var Gribblesval = [];
        var Hospitalval = [];
        var Months = [];


        $.ajax({
            url: "../Store/GribblesDashboard/shareingribble",
            data: { Fromdate: drop },
            type: 'POST',
            dataType: 'json',
            success: function (result) {
                $.each(result, function (ui, item) {

                    Gribblesval.push(parseInt((item.BedID).toString().substr(0, 2)));
                    Hospitalval.push(parseInt((item.AdvAmt).toString().substr(0, 2)));
                    Months.push(item.BaseEntityName);

                   
                });

                vibrant.setOptions({
                    colors: ['rgb(8, 249, 235)', 'rgb( 15, 8, 249 )', '#08F9EB', '#0F08F9']
                });

                $('#shareingribblehospitalid1').vibrant({
                    chart: {
                        type: 'column',
                        height: 230

                    },
                    xAxis: {
                        categories: Months,
                        labels: {
                            rotation: -45,
                            style: {
                                fontSize: '10px',
                                fontFamily: 'Verdana, sans-serif'
                            }
                        }
                    },
                    yAxis: {
                        title: {
                            text: null
                        },
                        labels: {
                            formatter: function () {
                                return this.value + '%';
                            }
                        }
                    },
                    title: {
                        text: ''
                    },
                    subtitle: {
                        text: ''
                    },
                    plotOptions: {
                        column: {
                            pointPadding: 0.2,
                            borderWidth: 0,
                            dataLabels: {
                                enabled: true,
                                format: ' {point.y:0f}'
                            }
                        }
                    },

                    tooltip: {
                        headerFormat: '<span style="font-size:10px">{point.key}</span><table>',
                        pointFormat: '<tr><td style="color:{series.color};padding:0">{series.name}: </td>' +
                                           '<td style="padding:0"><b>{point.y:.0f}% </b></td></tr>',
                        footerFormat: '</table>',
                        shared: true,
                        useHTML: true
                    },
                    series: [{
                        name: 'Gribble',
                        data: Gribblesval

                    }
                , {
                    type: 'spline',
                    dataLabels: {
                        enabled: true,
                        format: ' {point.y:0f}'
                    },
                    name: 'Hospital',
                    data: Hospitalval,
                    marker: {
                        lineWidth: 2,
                        fillColor: 'Blue'
                    }



                }]



                });

            }

        });
    }

