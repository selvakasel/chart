<script type="text/javascript">



    $("#MenuNamepartialview").text('Gribbles Dashboard');

    //    var currentmonths = TodayDate.getMonth();
    var currentmonths = (new Date).getMonth();
    $("#months").val(currentmonths);


    $("#drop").change(function () {

        var drop = $("#drop").val();
        Shareingribblehospitalfun(drop);

    });


    $("#drop1").change(function () {

        var drop1 = $("#drop1").val();
        Drselvarajstatsticsfun(drop1);

    });

    $("#drop2").change(function () {

        var drop2 = $("#drop2").val();
        Salutiationstatsticsfun(drop2);

    });
    $("#drop3").change(function () {

        var drop3 = $("#drop3").val();
        Newdoctorsactivatedfun(drop3);

    });
    $("#drop4").change(function () {

        var drop4 = $("#drop4").val();
        Lostdoctorstatisticsfun(drop4);

    });
    var m = 0;

    $("#ft1").hide(); $("#ft2").hide(); $("#ft3").hide(); $("#ft4").hide();
    $("#departmenttestanalysisbtn,#statewisetestanalysisbtn,#racepatientsbtn,#lostdoctorstatisticsbtn,#drlabonedoctoranalysisbtn").click(function () {


        $("#myModal2").modal('show');
        $("#myModal2").modal({ backdrop: false });

        $("#frmtdate11").pickadate({
            format: "dd/mm/yyyy",
            // min: true,
            selectMonths: true,
            selectYears: true,
            editable: 'ray'

        });
        $("#todate1").pickadate({
            format: "dd/mm/yyyy",
            //  min: true,
            selectMonths: true,
            selectYears: true,
            editable: 'ray'

        });
        m = $(this).val();

    });


    var fromdate;
    var todate;

    $("#btnsav1").click(function () {


        fromdate = $("#frmtdate11").val();
        todate = $("#todate1").val();

        if (parseInt(m) == 1) {
            $("#ft1").show();

            $("#frmd1").val(fromdate);
            $("#tod1").val(todate);

            Departmenttestanalysisfun();
            fromdate = $("#frmtdate11").val('');
            todate = $("#todate1").val('');

        }
        else if (parseInt(m) == 2) {
            $("#ft2").show();
            $("#frmd2").val(fromdate);
            $("#tod2").val(todate);

            Statewisetestanalysisfun();
            fromdate = $("#frmtdate11").val('');
            todate = $("#todate1").val('');
        }
        else if (parseInt(m) == 3) {
            $("#ft3").show();
            $("#frmd3").val(fromdate);
            $("#tod3").val(todate);

            Racepatientsfun()
            fromdate = $("#frmtdate11").val('');
            todate = $("#todate1").val('');

        }
        else if (parseInt(m) == 5) {
            $("#ft4").show();
            $("#frmd4").val(fromdate);
            $("#tod4").val(todate);

            Drlabonedoctoranalysisfun()
            fromdate = $("#frmtdate11").val('');
            todate = $("#todate1").val('');

        }


        m = 0;
        $("#myModal2").modal('hide');
    });


    $("#cancel2").click(function () {
        $(".modal fade").modal('hide');
    });



</script>
<script type="text/javascript">


    function mydate(dates) {
        var dob = dates;
        var splitdob = dob.split('/');
        var td = splitdob[2] + '-' + splitdob[1] + '-' + splitdob[0];
        return td;
    }


    function Departmenttestanalysisfun() {

        $.ajax({
            url: "../Store/GribblesDashboard/Departmenttestanalysis",
            data: { Fromdate: fromdateconv(fromdate), Todate: todateconv(todate) },
            type: 'POST',
            dataType: 'json',
            success: function (result) {

                var Deptname = [];
                var Deptval = [];
                $.each(result, function (ui, item) {
                    Deptname.push(item.BaseEntityName);
                    Deptval.push(item.BedID);
                });
                $('#departmenttestanalysisid1').vibrant({
                    chart: {
                        type: 'bar',
                        height: 230
                    },

                    title: {
                        text: ''
                    },
                    subtitle: {
                        text: '</a>'
                    },
                    xAxis: {
                        categories: Deptname,
                        labels: {
                            // rotation: -55,
                            style: {
                                fontSize: '10px',
                                fontFamily: 'Verdana, sans-serif'
                            }
                        }
                    },
                    yAxis: {
                        min: 0,
                        title: {
                            text: '',
                            align: 'high'
                        },
                        labels: {
                            overflow: 'justify'
                        }
                    },
                    tooltip: {
                        valueSuffix: ' '
                    },
                    plotOptions: {
                        bar: {
                            dataLabels: {
                                enabled: true
                            }
                        }
                    },

                    credits: {
                        enabled: false
                    },
                    series: [{
                        name: 'Department',
                        color: '#FF7F50',
                        data: Deptval
                    }]



                });

            }

        });
    }


