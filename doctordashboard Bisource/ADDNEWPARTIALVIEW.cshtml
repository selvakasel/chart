<script type="text/javascript">

    $("#newtab").hide();


    $("#MenuNamepartialview").text('Doctor Dashboard');

    $("#ft9").hide();
    $("#ft14").hide();

    $("#Patientwaitingtimesbtn,#Patientwaitingintervalbtn").click(function () {



        $("#myModal1").modal('show');
        $("#myModal1").modal({ backdrop: false });

        $("#frmtdate12").pickadate({
            format: "dd/mm/yyyy",
            // min: true,
            selectMonths: true,
            selectYears: true,
            editable: 'ray'

        });

        m = $(this).val();



    });


    var fromdate;

    $("#btnsav2").click(function () {


        fromdate = $("#frmtdate12").val();

        debugger
        if (parseInt(m) == 9) {

            $("#ft9").show();
            $("#frmd9").val(fromdate);

            Patientwaitingtimesfun();
            fromdate = $("#frmtdate12").val('');
        }
        else if (parseInt(m) == 14) {

            $("#ft14").show();
            $("#frmd14").val(fromdate);

            Patientwaitingintervalfun();
            fromdate = $("#frmtdate12").val('');
        }
        m = 0;

        $("#myModal1").modal('hide');
    });

</script>
